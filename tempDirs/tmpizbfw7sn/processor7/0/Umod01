FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      Umod01;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inletWater
    {
        type            fixedValue;
        value           uniform (1 0 0);
    }
    outletWater
    {
        type            zeroGradient;
    }
    freeSurface
    {
        type            slip;
    }
    leftSide
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    rightSide
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    bedWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary7to9
    {
        type            processor;
        value           uniform (1 0 0);
    }
}


